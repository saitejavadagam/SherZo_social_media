{
  "builds": [{
      "src": "sherzo/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
  }],
  "routes" : [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "sherzo/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles_build"
}
